// stage content:
(lib.passorblockia = function(mode,startPosition,loop) {
	this.initialize(mode,startPosition,loop,{});

	// timeline functions:
	this.frame_0 = function() {
		// note, there are a few function calls from within the packet animation
		// to trigger things at a specific time.  If you find the stuffed bear you get the prize.
		this.points_mc.visible = false;
		this.bubble_mc.visible = false;
		this.packetAni_mc.visible = false;
		//   "p" is the global stage in createjs.. so this just makes these objects
		//   accessible even from deep down in a sub animation clip like the packet
		p.points_mc = this.points_mc;
		p.packetAni_mc = this.packetAni_mc;
		p.score_txt = this.score_txt;
		p.bubble_mc = this.bubble_mc;
		p.start_btn = this.start_btn;
		p.reset_btn = this.reset_btn;
		p.pass_btn = this.pass_btn;
		p.block_btn = this.block_btn;
		p.explosion = this.explosion;
		p.timer = null;
		p.passCount = 0;
		p.count = 5;
		
		
		// setting dynamic text with the value loaded from react
		//.. a way to get the text behind the vector graphic drawing of the envelope
		this.internet_txt.text = getMediaText("pass-or-block-ia-ID_txt04--UUID");
		this.isp_txt.text = getMediaText("pass-or-block-ia-ID_txt03--UUID");
		
		// used so we can find the text divs to swap their color via css
		this.start_btn.textID = "pass-or-block-ia-start--UUID"
		this.reset_btn.textID = "pass-or-block-ia-reset--UUID"
		this.pass_btn.textID = "pass-or-block-ia-pass--UUID"
		this.block_btn.textID = "pass-or-block-ia-block--UUID"
		
		
		/* function for IP Address with pass and block status*/
		/*************************************************************/
		
		//Variables declaration
		var octNum1;
		var octNum2;
		var octNum3;
		var octNum4;
		var correctAnswer;
		var userAnswer;
		var btnClicked;
		var stopGame = false;
		var count;
		var IPaddress;
		var curRouterStatus = 'CLEAR';
		function getOctect1() {
			octNum1 = Number(Math.random() * 223);
			if (octNum1 == 0) {
				octNum1 = Number(Math.random() * 223);
			}
		}
		function getOctect2() {
			octNum2 = Number(Math.random() * 255);
			if (octNum2 == 0) {
				octNum2 = Number(Math.random() * 255);
			}
		}
		function getOctect3() {
			octNum3 = Number(Math.random() * 255);
			if (octNum3 == 0) {
				octNum3 = Number(Math.random() * 255);
			}
		}
		function getOctect4() {
			octNum4 = Number(Math.random() * 254);
			if (octNum4 == 0) {
				octNum4 = Number(Math.random() * 254);
			}
		}
		
		// function for check IP address
		
		function chkValidity() {
			//console.log("     IPaddress: " + IPaddress);
			if ((octNum1 == 10) || ((octNum1 == 192) && (octNum2 == 168)) || ((octNum1 == 172) && ((octNum2 > 15) && (octNum2 < 32)))) {
				correctAnswer = "BLOCK";
			} else {
				correctAnswer = "PASS";
				p.passCount++;
			}
			if(p.passCount > 2){
				p.passCount = 1;
				getOctet();
			}
			if(Math.round(octNum1) == 127 || Math.round(octNum1) == 0){
				getOctet();
			}
		}
		
		
		function getOctet() {
		
			getOctect1();
			getOctect2();
			getOctect3();
			getOctect4();
			//console.log("getOctet " + octNum1);
			if ((octNum1 > 3) && (octNum1 < 14)) {
				octNum1 = 10;
			}
			if ((octNum1 > 26) && (octNum1 < 34)) {
				octNum1 = 10;
			}
			if ((octNum1 > 56) && (octNum1 < 64)) {
				octNum1 = 10;
			}
			if ((octNum1 > 183) && (octNum1 < 194)) {
				octNum1 = 192;
				octNum2 = 168;
			}
			if ((octNum1 > 205) && (octNum1 < 208)) {
				octNum1 = 192;
				octNum2 = 168;
			}
			if ((octNum1 > 155) && (octNum1 < 168)) {
				octNum1 = 192;
				octNum2 = 168;
			}
			if ((octNum1 > 170) && (octNum1 < 176)) {
				octNum1 = 172;
				octNum2 = Number(Math.random() * 31);
				if ((octNum2 > 15) && (octNum2 < 31)) {
					octNum2 = octNum2 + Number(Math.random() * 15);
				}
			}
			if ((octNum1 > 150) && (octNum1 < 156)) {
				octNum1 = 172;
				octNum2 = Number(Math.random() * 31);
				if ((octNum2 > 15) && (octNum2 < 31)) {
					octNum2 = octNum2 + Number(Math.random() * 15);
				}
			}
			if ((octNum1 > 80) && (octNum1 < 86)) {
				octNum1 = 172;
				octNum2 = Number(Math.random() * 31);
				if ((octNum2 > 15) && (octNum2 < 31)) {
					octNum2 = octNum2 + Number(Math.random() * 15);
				}
			}
			IPaddress = Math.round(octNum1) + "." + Math.round(octNum2) + "." + Math.round(octNum3) + "." + Math.round(octNum4);
		
			chkValidity(); //Give value in PASS and BLOCK
		}
		
		
		
		
		//   -----   global functions
		//  this section is about 90% original actionscript code from prior to 2009
		p.getScore = function () {
			if (p.btnClicked == true) {
				if (p.userAnswer == correctAnswer) {
					if (correctAnswer == "PASS") {
						p.points_mc.visible = true;
						p.count = p.count + 1;
						p.packetAni_mc.gotoAndPlay(106);
						p.points_mc.ball.gotoAndStop(0);
					} else {
						this.points_mc.visible = true;
						p.count = p.count + 1;
						p.packetAni_mc.gotoAndPlay(137);
						p.points_mc.ball.gotoAndStop(0);
					}
				} else {
					if (p.userAnswer == "PASS") {
						this.points_mc.visible = true;
						this.packetAni_mc.gotoAndPlay(106);
					} else {
						this.points_mc.visible = true;
						this.packetAni_mc.gotoAndPlay(137);
					}
					p.explosion.visible = true;
					p.explosion.gotoAndPlay(1);
					p.count = p.count - 1;
					//console.log("count: " + count);
					this.points_mc.ball.gotoAndStop(6);
					
				}
			} else {
				p.explosion.visible = true;
				p.explosion.gotoAndPlay(1)
				p.points_mc.visible = true;
				p.packetAni_mc.visible = false;
				p.count = p.count - 1;
				p.points_mc.ball.gotoAndStop(6);
			}
			this.points_mc.gotoAndPlay(2);
		}
		
		
		function resetAll() {
			isGameOver();
			//console.log("resetAll  stopGame: " + stopGame);
			if (stopGame == true) {
				if (p.count == 10) {
					//_this.feedBack.titleTextId="ID_MsgGoodTitle";
					//_this.feedBack.bodyTextId="ID_MsgGoodBody";
					//alert("YOUWON");
					window.notify({ type: 'success' });
				} else {
					// alert("BETTER TRY NEXT TIME");
					//_this.feedBack.titleTextId="ID_MsgBadTitle";
					//_this.feedBack.bodyTextId="ID_MsgBadBody";
					window.notify({ type: 'error' });
				}
				p.packetAni_mc.visible = false;
			} else {
				p.packetAni_mc.visible = true;
				p.bubble_mc.visible = false;
				p.points_mc.visible = false;
				p.packetAni_mc.gotoAndStop(1);
				p.btnClicked = false;	    
				getOctet();
				p.packetAni_mc.gotoAndPlay(1)
				p.packetAni_mc.Packet_mc.IpAddress_txt.text = IPaddress;
			}
			p.pass_btn.gotoAndStop(0);
			p.block_btn.gotoAndStop(0);
			document.getElementById(p.pass_btn.textID).style.color = "#000";
			document.getElementById(p.block_btn.textID).style.color = "#000";
			resetRouterButtons();
			p.explosion.visible = false;
		}
		
		function resetRouterButtons(){
			p.pass_btn.mouseEnabled = true;
			p.block_btn.mouseEnabled = true;
			p.pass_btn.gotoAndStop(0);
			p.block_btn.gotoAndStop(0);
			document.getElementById(p.pass_btn.textID).style.color = "#000";
			document.getElementById(p.block_btn.textID).style.color = "#000";
		}
		function disableRouterButtons(){
			p.pass_btn.mouseEnabled = false;
			p.block_btn.mouseEnabled = false;
		}
		
		function resetandReady() {
			window.dismissNotification();
			clearInterval(p.timer);
			p.timer = null;
			p.userAnswer = null;
			stopGame = false;
			p.btnClicked = false;
			p.score_txt.text = 5;
			p.start_btn.gotoAndStop(0);
			p.reset_btn.mouseEnabled = false;
			p.reset_btn.gotoAndStop(0);
			document.getElementById(p.reset_btn.textID).style.color="#000";
			resetRouterButtons();
			disableRouterButtons();
			document.getElementById(p.start_btn.textID).style.color = "#000";
			p.start_btn.mouseEnabled = true;
			p.count = 5;
			p.score_txt.text = p.count.toString();
			p.packetAni_mc.visible = false;
			p.packetAni_mc.gotoAndStop(0);
			p.bubble_mc.visible = false;
			p.points_mc.visible = false;
			p.points_mc.ball.gotoAndStop("none");
			//get first IP Address
			getOctet();
			p.explosion.visible = false;
		}
		
		p.callTimer = function () {
			if(p.timer == null){
				p.timer = setInterval(onTimer, 2000);
			}
		}
		
		function onTimer() {
			clearInterval(p.timer);
			p.timer = null;
			resetAll();
		}
		
		function isGameOver() {
			//console.log("isGameOver");
			if (p.count == 10 || p.count == 0) {
				stopGame = true;
			}
		}
		//    -----   end global functions  
		
		
		//   -----   button actions
		// this is all new.. I couldn't get the buttonHelper in createjs to work but it wouldn't have helped much anyway.  poser.
		this.stage.enableMouseOver();
		this.start_btn.cursor = this.reset_btn.cursor = this.block_btn.cursor = this.pass_btn.cursor = "pointer";
		
		this.start_btn.addEventListener("click", fl_start.bind(this));
		this.reset_btn.addEventListener("click", fl_reset.bind(this));
		this.pass_btn.addEventListener("click", fl_pass.bind(this));
		this.block_btn.addEventListener("click", fl_block.bind(this));
		this.pass_btn.mouseEnabled = false;
		this.block_btn.mouseEnabled = false;
		this.reset_btn.mouseEnabled = false;
		
		function fl_start() {
			getOctet();
			this.packetAni_mc.Packet_mc.IpAddress_txt.text = IPaddress;
			this.start_btn.gotoAndStop(3);
			this.start_btn.mouseEnabled = false;
			this.pass_btn.mouseEnabled = true;
			this.block_btn.mouseEnabled = true;
			this.reset_btn.mouseEnabled = true;
			this.packetAni_mc.visible = true;
			this.packetAni_mc.gotoAndPlay(1);
			this.packet_mc.visible = false;
			this.stage.update();
			//this.start_btn.enableMouseOver = false;
			document.getElementById(this.start_btn.textID).style.color = "#FFF";
		}
		function fl_reset() {
			//console.log("reset");
			resetandReady();
		}
		
		function fl_pass(e) {
			//console.log("pass");
			this.pass_btn.gotoAndStop(3);
			document.getElementById(this.pass_btn.textID).style.color = "#FFF";
			p.btnClicked = true;
			p.userAnswer = "PASS";
			p.bubble_mc.visible = false;
			disableRouterButtons();
		}
		function fl_block() {
			//console.log('block');
			this.block_btn.gotoAndStop(3);
			document.getElementById(this.block_btn.textID).style.color = "#FFF";
			p.btnClicked = true;
			p.userAnswer = "BLOCK";
			p.bubble_mc.visible = true;
			disableRouterButtons();
		}
		
		
		this.reset_btn.addEventListener("mouseover", fl_MouseOverHandler.bind(this.reset_btn));
